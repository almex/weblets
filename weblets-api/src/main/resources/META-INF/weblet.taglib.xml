<?xml version="1.0"?>
<facelet-taglib version="2.0"
                xmlns="http://java.sun.com/xml/ns/javaee"
                xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facelettaglibrary_2_0.xsd">
    <namespace>http://weblets.dev.java.net/facelet-tags</namespace>

    <function>
        <function-name>resource</function-name>
        <function-class>net.java.dev.weblets.FaceletELWebletUtils</function-class>
        <function-signature>java.lang.String getResource( java.lang.String, java.lang.String)</function-signature>
    </function>

    <function>
        <function-name>extResource</function-name>
        <function-class>net.java.dev.weblets.FaceletELWebletUtils</function-class>
        <function-signature>java.lang.String getResource(java.lang.String, java.lang.String, boolean)
        </function-signature>
    </function>


    <function>
        <function-name>url</function-name>
        <function-class>net.java.dev.weblets.FaceletELWebletUtils</function-class>
        <function-signature>java.lang.String getURL( java.lang.String, java.lang.String)</function-signature>
    </function>

    <function>
        <function-name>extUrl</function-name>
        <function-class>net.java.dev.weblets.FaceletELWebletUtils</function-class>
        <function-signature>java.lang.String getURL(java.lang.String, java.lang.String, boolean)
        </function-signature>
    </function>

    <function>
        <function-name>isResourceLoaded</function-name>
        <function-class>net.java.dev.weblets.FaceletELWebletUtils</function-class>
        <function-signature>boolean isResourceLoaded(java.lang.String,java.lang.String)</function-signature>
    </function>

    <function>
        <function-name>jsfResource</function-name>
        <function-class>net.java.dev.weblets.FaceletELWebletUtils</function-class>
        <function-signature>java.lang.String getJSFResource(java.lang.String, java.lang.String)</function-signature>
    </function>

    <function>
        <function-name>jsfURL</function-name>
        <function-class>net.java.dev.weblets.FaceletELWebletUtils</function-class>
        <function-signature>java.lang.String getJSFURL(java.lang.String, java.lang.String)</function-signature>
    </function>


</facelet-taglib>
